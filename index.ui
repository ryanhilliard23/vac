    border: 1px solid #2b2b2b;   /* Subtle border for the container */
}

/* 2. The Sliding Pill (Active Highlight) */
/* Using QFrame selector ensures the radius is applied */
QFrame#anim_pill {
    background-color: #0f0f12;   /* Your requested active color */
    border-radius: 18px;         /* radius must be &lt; container radius to fit */
    border: 1px solid #333333;   /* Border around the pill */
}

/* 3. The Buttons */
#btnNavDashboard, #btnNavHistory {
    background: transparent;     
    border: none;
    color: #888888;              
    font-weight: bold;
    font-size: 14px;
    border-radius: 18px;         /* Match the pill radius */
}

/* 4. Text Colors */
#btnNavDashboard:hover, #btnNavHistory:hover {
    color: #bbbbbb;
}

