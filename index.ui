                 <property name="styleSheet">
                  <string notr="true">/* --- QComboBox Base Style --- */
QComboBox {
    background-color: #0F0F12;      /* matches bg-surface */
    border: 1px solid #2A2A30;      /* matches border-white/5 */
    border-radius: 12px;            /* matches rounded-xl */
    padding: 10px 16px;             /* matches py-3 px-4 */
    color: #9ca3af;                 /* matches text-gray-400 */
    font-family: &quot;Manrope&quot;, sans-serif;
    font-size: 13px;                /* matches text-sm */
    font-weight: 500;
}

/* --- Hover State --- */
QComboBox:hover {
    background-color: #1A1A1F;      /* matches hover:bg-surfaceHighlight */
    color: #ffffff;                 /* lighter text on hover */
    border: 1px solid #3f3f46;
}

/* --- Focus / Open State --- */
QComboBox:on { 
    border: 1px solid rgba(59, 130, 246, 0.5); /* matches focus:border-primary/50 */
    background-color: #0F0F12;
}

/* --- The Dropdown Button Area --- */
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 40px; 
    border-left-width: 0px;
    border-top-right-radius: 12px;
    border-bottom-right-radius: 12px;
    background: transparent;        /* Keep transparent to show parent bg */
}

/* --- The Arrow Icon --- */
/* Note: You need a PNG icon in your resource file for this to look perfect.
   If you don't have one, Qt uses a default Windows arrow. */
QComboBox::down-arrow {
    image: url(:/images/caret-down.png); /* Replace with your actual resource path */
    width: 14px;
    height: 14px;
    padding-right: 16px; /* Offset to match the HTML look */
}

/* --- The Popup List (The part that opens up) --- */
QComboBox QAbstractItemView {
    background-color: #0F0F12;      /* Dark background */
    border: 1px solid #2A2A30;      /* Thin border */
    selection-background-color: #1A1A1F; /* Hovered Item Background */
    selection-color: #ffffff;       /* Hovered Item Text */
    outline: 0px;                   /* Remove dotted selection line */
    padding: 4px;
    border-radius: 8px;
    color: #9ca3af;
}</string>
