                <property name="styleSheet">
                 <string notr="true">/* --- Main Download Button (Gradient Style) --- */
QPushButton#btnStartDownload {
    /* Gradient Background: Matches 'bg-gradient-to-b from-primary to-blue-700' */
    background-color: qlineargradient(
        spread:pad, x1:0.5, y1:0, x2:0.5, y2:1, 
        stop:0 #3B82F6, 
        stop:1 #1d4ed8
    );
    
    /* Border &amp; Radius */
    border: 1px solid rgba(255, 255, 255, 0.1);
    border-radius: 16px;  /* Matches 'rounded-2xl' */
    
    /* Text Styling */
    color: white;
    font-family: &quot;Manrope&quot;, sans-serif;
    font-size: 18px;      /* Large text like the HTML &lt;h2&gt; */
    font-weight: bold;
    text-align: left;
    /* Spacing */
    padding: 20px;        /* Large padding to mimic the card size */
}

/* --- Hover State --- */
QPushButton#btnStartDownload:hover {
    /* Slightly lighter gradient to mimic the scale/glow effect */
    background-color: qlineargradient(
        spread:pad, x1:0.5, y1:0, x2:0.5, y2:1, 
        stop:0 #60a5fa,   /* Lighter Blue */
        stop:1 #2563eb    /* Blue-600 */
    );
    border: 1px solid rgba(255, 255, 255, 0.3); /* Brighter border */
}

/* --- Pressed State --- */
QPushButton#btnStartDownload:pressed {
    /* Darker solid color or inverted gradient */
    background-color: #1e40af; /* Blue-800 */
    border: 1px solid transparent;
    
    /* Slight shift to mimic physical press */
    padding-top: 22px;
    padding-bottom: 18px;
}

/* --- Disabled State (Optional) --- */
QPushButton#btnStartDownload:disabled {
    background-color: #1A1A1F;
    color: #52525b;
    border: 1px solid #27272a;
}</string>
